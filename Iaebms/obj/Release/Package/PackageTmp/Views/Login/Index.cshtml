@{
    ViewBag.Title = "登录";
}
<style type="text/css">
    div {
        align-content:center;
    }   
    #tab {
        border-collapse:inherit;
    } 
</style>
<div>
    <div>
        <div>
            <table id="tab" cellpadding="1" cellspacing="1">
               <tr>
                    <td>NAME</td>
                    <td><input name="usname" type="text" id="usname" /></td>
               </tr>
               <tr>
                   <td>PASSWORD</td>
                   <td><input name="paword" type="text" id="paword" /></td>
               </tr>
               <tr>
                   <td><input name="paword" type="button" value="LOGIN" onclick="test()"/></td>
                   <td><input name="paword" type="button"  value="REST"/></td>
               </tr>
            </table> 
        </div>
    </div>
</div>
<script src="../Js/tl/jsencrypt.js" type="text/javascript"></script>
<script src="../Js/tl/md_crypt.js" type="text/javascript"></script>
<script type="text/javascript">
    function test() {
        var strcs = hex_md5($("#paword").val()).toUpperCase();
        $.getJSON("../Json/c36853ea059e0d71a67557e5ee54b835/059e0d71a67557e5.json", function (data) {
            var encrypt = new JSEncrypt();
            encrypt.setPublicKey(data["q4c2cd3d41b221e3f"]);
            strcs = '{"um":"' + $("#usname").val() + '","pd":"' + strcs + '"}';
            var sss = encrypt.encrypt(strcs);
            //console.log("sss:" + sss);
            $.ajax({
                url: "../Login/Info",
                type: "post",
                //data: '{"result":"' + sss + '"}',
                data: sss,
                dataType: "text",
                success: function (data) {
                    //console.log(data)
                }, error: function () {

                }
            });
        }); 
    }
</script>